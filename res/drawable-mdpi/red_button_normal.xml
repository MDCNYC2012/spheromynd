<?xml version="1.0" encoding="utf-8"?>

<layer-list
	xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape>
			<padding
				android:left="1dp"
				android:top="1dp"
				android:right="1dp"
				android:bottom="1dp" />
			<solid
				android:color="#AD5E61" />
			<corners
				android:radius="3dp" />
		</shape>
	</item>

	<item>
		<shape>
			<padding
				android:left="2dp"
				android:top="2dp"
				android:right="2dp"
				android:bottom="2dp" />
			<gradient
				android:startColor="#e5e5e5"
				android:endColor="#DFB9B8"
				android:angle="270" />
			<corners
				android:radius="3dp" />
		</shape>
	</item>
</layer-list>